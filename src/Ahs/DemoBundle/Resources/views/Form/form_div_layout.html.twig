
{# This Form Theme overrides vendor/symfony/symfony/src/Symfony/Bridge/Twig/Resources/views/Form/form_div_layout.html.twig #}


{% block email_widget %}
    {% spaceless %}
        {% for child in form %}
            {{ form_row(child) }}
        {% endfor %}
    {% endspaceless %}
{% endblock %}

{% block address_widget %}
    {% spaceless %}
        {{ form_row(form.adrStreet) }}
        {{ form_widget(form.cty) }}
    {% endspaceless %}
{% endblock %}

{% block city_widget %}
    {% spaceless %}
        {% for child in form %}
            {{ form_row(child) }}
        {% endfor %}
    {% endspaceless %}
{% endblock %}
